{
  "name": "@iviva/react-tsdoc",
  "version": "0.2.5",
  "description": "Tool to generate documentation and type definitions for your React component library",
  "keywords": [
    "react",
    "documentation",
    "typescript",
    "tsdoc"
  ],
  "homepage": "https://github.com/lucy-platform/react-tsdoc",
  "license": "ISC",
  "author": "",
  "main": "dist/cli/index.js",

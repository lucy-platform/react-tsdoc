    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@types/mkdirp": "^1.0.1",
    "@types/node": "^14.6.0",
    "ts-node": "^10.9.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucy-platform/react-tsdoc.git"
  },
  "bugs": {
    "url": "https://github.com/lucy-platform/react-tsdoc/issues"
  },
  "files": [
    "dist/**/*",
    "bin/**/*"

{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Debug react-tsdoc CLI",
            "program": "${workspaceFolder}/bin/react-tsdoc.js",
            "args": [
                "docs",
                "./test/sample-app/src/index.tsx",
                "./test/dist/docs/",
                "--module-name",
                "my-types"
            ],
            // "args": [
            //     "types",
            //     "./test/sample-app/src/index.tsx",
            //     "./test/dist/types/types.d.ts",
            //     "--module-name",
            //     "my-types"
            // ],
            "skipFiles": [
                "<node_internals>/**"
            ],
            "console": "integratedTerminal"
        }
    ]
}